/* Modal Styles
   ------------------------------------------------------------------------- */

.modal-content {
    background: #fff;
    width: 370px;
    margin: 0;
    padding: 25px 30px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    -ms-border-radius: 5px;
    -o-border-radius: 5px;
    border-radius: 5px;
}

.modal-content .primary-heading {
    margin: 0 0 20px;
}

.modal-content .primary-heading h1 {
    font-size: 26px;
    color: #444;
    margin: 0;
    line-height: 26px;
}

.modal-content .flush {
    padding: 0;
    margin-left: -30px;
    margin-right: -30px;
    clear: both;
}

.module-set {
    background: #f7f7f7;
    padding: 30px;
    -webkit-box-shadow: rgba(0, 0, 0, 0.07) 0 0 0 1px inset;
    -moz-box-shadow: rgba(0, 0, 0, 0.07) 0 0 0 1px inset;
    box-shadow: rgba(0, 0, 0, 0.07) 0 0 0 1px inset;
}

.module-set .urlTittle {
    margin: 30px 0 10px;
}

.modal-content .form-footer {
    float: left;
    padding: 15px 30px;
    line-height: 110%;
    clear: both;
    -webkit-border-radius: 0 0 5px 5px;
    -moz-border-radius: 0 0 5px 5px;
    -ms-border-radius: 0 0 5px 5px;
    -o-border-radius: 0 0 5px 5px;
    border-radius: 0 0 5px 5px;
}

.modal-content #dataContentModal {
    text-align: center;
}

#termsModal, #policyModal {
    width: 430px;
    padding: 0;
    margin: -250px 0 0 -215px;
}

/* Forms Styles
   ------------------------------------------------------------------------- */

.formTittle {
    padding: 0;
    margin: 0 10px;
    color: #444;
    font-size: 22px;
}

.mainFormBox {
    width: 460px;
    margin: 120px 240px 0;
    float: left;
}

.formBox {
    width: 440px;
    float: left;
}

.formTextInfo {
    margin: 0 10px;
    color: #666;
    font-size: 13px;
}

#loginTwitter {
    margin: 15px 14px 15px 10px;
    min-width: 172px;
    float: left;
}

#loginFacebook {
    margin: 15px 10px 15px 14px;
    min-width: 172px;
    float: left;
}

#loginFormItem, #registerFormItem, #forgetPasswordForm, #registerForm {
    margin: 10px 20px;
}

.formBox input {
    width: 386px;
    height: 25px;
}

.formBox label {
    font-weight: bold;
}

.error {
    color: red;
}

.formBox button {
    float: left;
    margin: 0 0 10px;
}

.optionLoginText {
    float: left;
    margin: 10px 10px 20px;
    color: #666;
    font-size: 13px;
}

.termsPolicyText {
    float: left;
    margin: 10px;
    color: #666;
    font-size: 13px;
}

#errorContainer {
    padding: 100px 95px 20px;
    float: left;
}

#errorContainer img {
    padding: 0 200px;
}

#errorContainer h1 {
    color: #666;
    text-align: center;
}

#errorContainer p {
    font-size: 16px;
    text-align: center;
    margin: 20px 0 80px;
}
