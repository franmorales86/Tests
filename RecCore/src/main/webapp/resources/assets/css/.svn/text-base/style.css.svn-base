/* Styles Commons
   ------------------------------------------------------------------------- */
*, *:focus {
    outline: 0 none;
}

body {
    background: #f0f0ed;
}

.box {
    background: none repeat scroll 0 0 #FFFFFF;
    padding: 10px;
    border-radius: 5px 5px 5px 5px;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.07);
}

/* Header
   ------------------------------------------------------------------------- */
#navbarHeader {
    /* 	background: none repeat scroll 0 0 #333333; */
    background-color: #2D3538;
    box-shadow: 0 0 7px 0 rgba(50, 50, 50, 0.45);
    font-size: 13px;
}

#navbarHeader .logoNav a img {
    max-width: 100%;
    height: auto;
    padding: 0;
    margin: 5px 0;
}

/* Styles Login - Register
   ------------------------------------------------------------------------- */
/* Styles Buttons
 ------------------------------------------------------------------------- */

.button {
    text-decoration: none !important;
    text-shadow: none !important;
    font-weight: bold;
    padding: 13px 15px;
    background: #4090ba;
    color: #fff !important;
    line-height: 1 !important;
    font-size: 14px !important;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    white-space: nowrap;
    border: none;
    cursor: pointer;
    display: -moz-inline-stack;
    display: inline-block;
    vertical-align: middle;
    zoom: 1;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    border-radius: 4px;
}

.button:hover {
    text-decoration: none;
    color: #fff;
    background: #3981a7;
}

.button-small {
    font-size: 12.5px !important;
    padding: 11px 12px !important;
}

.button-green {
    background-color: #82b964;
}

.button-green:hover {
    background: #74b152;
}

.button-red {
    background: #ea6c6c;
}

.button-red:hover {
    background: #ea545b;
}

.button-disabled, .button-disabled:hover {
    background-color: #e6e6e6 !important;
    opacity: 0.65;
    color: #333333 !important;
    padding: 6px 12px !important;
    line-height: 20px;
    text-align: center;
    vertical-align: middle;
    border: 1px solid #bbbbbb;
    border-color: rgba(0, 0, 0, 0.15) rgba(0, 0, 0, 0.15) rgba(0, 0, 0, 0.25);
}

.button-close {
    position: absolute;
    top: 20px;
    right: 20px;
    background: #4090ba;
    padding: 5px 6px;
    line-height: 0;
    text-decoration: none;
    z-index: 100;
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    -ms-border-radius: 4px;
    -o-border-radius: 4px;
    border-radius: 4px;
}

.button-close i {
    width: 12px;
    height: 14px;
    color: #ffffff;
    line-height: 14px;
    font-size: 16px;
}

.button-close:hover {
    text-decoration: none;
    background: #ea6c6c;
}

.button-upload {
    padding: 10px 15px 6px !important;
    vertical-align: middle;
}

.button-upload i {
    margin: 0 5px 0 0;
    font-size: 22px;
    vertical-align: middle;
}

#choose-photo {
    text-align: center;
}

