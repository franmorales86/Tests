package com.reclabs.recomendar.common.types;

/**
 * Listado de las variables de entorno que se encuentran
 * en la tabla de SimpleDB (configDB)
 * @author rmorales
 */
public enum EnvVariableType {
	/**
	 * El último id de twitter que ha sido obtenido
	 * desde el Scout
	 */
	LAST_TWITTER_ID_LOADED("LAST_TWITTER_ID_LOADED");

	private EnvVariableType(final String id) {
		this.id = id;
	}

	private String id;

	/**
	 * @return Id de la variable de entorno
	 */
	public String getId() {
		return this.id;
	}
}
